#include <iostream>
#include <vector>
#include <map>
#include <set>
#include <cmath>
#include <algorithm>
#include <string>
#include <queue>
#include <stack>
#include <sstream>
#include <fstream>
#include <unordered_map>

using namespace std;


int main()
{
	int t; cin >> t;
	while (t--)
	{
		string num; cin >> num;
		if (num[0] == num[num.size() - 1]) cout << "YES" << endl;
		else cout << "NO" << endl;
	}
	return 0;
}
